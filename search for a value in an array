#include<stdio.h>
int main() {
    int a[10],n,find,found=0;
    printf("Enter the number of elements:");
    scanf("%d",&n);
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    printf("Enter the value you want to find: ");
    scanf("%d",&find);
    for(int i =0;i<n;i++){
        if(a[i]==find){
            found=1;
            break;
        }
    }
    if(found==1) {
        printf("Found");
    } else {
        printf("Not found");
    }
}
