#include<stdio.h>
int main() {
    int cards[11],n,insertvalue;
    printf("Enter the number of cards:");
    scanf("%d",&n);
    for(int i =0;i<n;i++){
        scanf("%d",&cards[i]);
    }
    printf("Enter the value to be inserted");
    scanf("%d",&insertvalue);
    for(int i=n;i>1;i--) {
        cards[i]=cards[i-1];
    }
    cards[1]=insertvalue;
    for(int i=0;i<n;i++) {
        printf("%d",cards[i]);
    }
}
