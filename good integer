#include<stdio.h>
int isgoodinteger(int n) {
    while(n>0) {
        int digit = n%10;
        if(digit%2!=0) {
            return 0;
            n/=10;
        }
        return 1;
    }
}
int goodnumber(int N) {
    int count =0,num=0;
    while(1){
        if(isgoodinteger(num)){
            count++;
            if(count==N)
            return num;
        }
        num++;
    }
}
int main() {
    int N;
    scanf("%d",&N);
    int result = goodnumber(N);
    printf("%d",result);
    return 0;
}
